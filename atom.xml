<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Right Brain Meme Pie</title>
 <link href="http://blog.ericbmerritt.com/atom.xml" rel="self"/>
 <link href="http://blog.ericbmerritt.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://blog.ericbmerritt.com/</id>
 <author>
   <name>Eric Merritt</name>
   <email>eric@merritt.tech</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://blog.ericbmerritt.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://blog.ericbmerritt.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
